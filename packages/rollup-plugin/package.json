{
  "name": "@traduki/rollup-plugin-traduki",
  "version": "0.6.1",
  "description": "Rollup plugin for Traduki",
  "author": "havelaer",
  "keywords": [
    "rollup-plugin",
    "messageformat",
    "pluralformat",
    "icu",
    "i18n",
    "L10n",
    "code-splitting",
    "translations"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/havelaer/traduki.git",
    "directory": "packages/rollup-plugin-traduki"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest tests",
    "build": "yarn run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.0.0",
    "@traduki/build-utils": "^0.6.0"
  },
  "devDependencies": {
    "@traduki/runtime": "^0.6.0",
    "@types/jest": "^26.0.4",
    "jest": "^26.1.0",
    "rollup": "^2.20.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
