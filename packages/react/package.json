{
  "name": "@traduki/react",
  "version": "0.5.0",
  "description": "Traduki React helpers",
  "author": "havelaer",
  "keywords": [
    "messageformat",
    "pluralformat",
    "icu",
    "i18n",
    "L10n",
    "translations",
    "react"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/havelaer/traduki.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib/*",
    "dev": "tsc -w -p tsconfig.cjs.json",
    "build": "yarn run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/react": "^16.9.43",
    "react": "^16.13.1",
    "rollup": "^2.22.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@traduki/runtime": "^0.5.0",
    "remarkable": "^2.0.1"
  }
}
