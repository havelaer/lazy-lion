    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "@traduki/build-utils": "^0.3.0",
    "loader-utils": "^2.0.0",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "@traduki/runtime": "^0.3.0",
    "@types/jest": "^26.0.4",
    "@types/loader-utils": "^2.0.1",
    "jest": "^26.1.0",
    "rollup": "^2.20.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
